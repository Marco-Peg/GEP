{
  "seed": 42,
  "dataset_name": "epipred",
  "device": "cuda",
  "n_epoch": 2,
  "lr": 1e-3,
  "decay_every": 2,
  "decay_rate": 0.5,
  "batch_size": 3,
  "sub_batch_size": 1,
  "lloss": 1,
  "pdrop": 0.1,
  "as_mesh": false,
  "n_points": 1000,
  "centered": true,
  "input_features": [
    "xyz",
    "bio",
    "hks"
  ],
  "hks_dim": 16,
  "precompute_data": false,
  "geo": true,
  "feature_transform": false,
  "PN_dims": [
    64,
    128
  ],
  "STN_incr_dims": [
    64,
    128
  ],
  "STN_decr_dims": [
    128,
    64
  ],
  "seg_dims": [
    64,
    32
  ],
  "losses_weight": {
    "BCE_ab": 1.0,
    "BCE_ag": 2.0
  },
  "metrics": [
    "MCC",
    "auroc",
    "accuracy",
    "precision",
    "recall"
  ]
}